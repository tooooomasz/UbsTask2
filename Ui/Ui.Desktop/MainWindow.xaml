<Window x:Class="tomaszbaginski.UbsTask2.Ui.Desktop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:desktop="clr-namespace:tomaszbaginski.UbsTask2.Ui.Desktop"
        mc:Ignorable="d"
        Title="My Fund" MinHeight="400" MinWidth="750" Height="600" Width="1000">
    <DockPanel LastChildFill="True">
        <desktop:NewStockForm DockPanel.Dock="Top" />
        <desktop:StocksSummary DockPanel.Dock="Right"/>
        <desktop:StocksView />
    </DockPanel>
</Window>
